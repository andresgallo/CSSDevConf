/** Documentation & Terms pages, hosted on Galactus - wrap them in this class. **/

.doc-styling {
    h1, h2, h3, h4, a, strong, .bold-line {
        font-family: TradeGothicBold;
        font-weight: normal;
    }
    h1 {
        font-size: 1.5em;
    }
    h2 {
        font-size: 1.5em;
        padding: 40px 0 10px 0;
    }
    h3 {
        font-size: 1.1em;
        padding-top: 10px;
    }
    .bold-line, strong {
        font-size: 1.05em;
    }
    p, ul, ol {
        line-height: 1.4em;
    }
    ul, ol {
        margin: 16px 0;
        padding-left: 40px;
    }
    pre, code {
        border: 1px solid #ddd;
        font-family: monospace;
    }
    code {
        font-size: 1em;
        padding: 2px 7px;
        background-color: #f4f4f4;
    }
    pre {
        padding: 20px;
        padding-left: 40px;
        white-space: pre-line;
        background-color: #fcfcfc;
        code {
            padding: 0;
            background-color: transparent;
            border: none;
        }
    }
    table {
        margin: 3px 0 30px 0;
        th {
            font-family: TradeGothicBold;
            line-height: 20px;
            font-weight: normal;
            background-color: #eee;
            border: 1px solid #ddd;
            padding-left: 5px;
        }
        td {
            border: 1px solid #ddd;
            padding: 4px 7px;
            font-size: 13px;
        }
        tr {
            line-height: 18px;
        }
        tr, th {
            vertical-align: top;
            font-size: 14px;
            text-align: left;
            &:nth-child(1) {
                width: 130px;
            }
            &:nth-child(2) {
                width: 190px;
            }
            &:nth-child(3) {
                width: 620px;
            }
        }
    }

    .image-box {
        margin-bottom: 30px;
        .box-wrapper {
            background-color: #f4f4f4;
            display: inline-block;
            h3.box-header {
                border: {
                    color: #ddd;
                    width: 1px 1px 0;
                    style: solid;
                }
                padding: 5px 10px;
            }
            .box-body {
                border: 1px solid #ddd;
                padding: 8px;
                > aside {
                    display: inline-block;
                    vertical-align: top;
                }
                aside.left {
                    margin-right: 5px;
                    min-width: 270px;
                }
                aside.right {
                    table {
                        background-color: #fff;
                        font-family: TradeGothicBold;
                        font-size: 1.1em;
                        margin: 0px;
                        td {
                            min-width: 130px;
                        }
                    }
                }
            }
        }
    }
}

