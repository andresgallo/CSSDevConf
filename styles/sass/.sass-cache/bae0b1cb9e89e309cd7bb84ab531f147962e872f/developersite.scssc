section.hide, div.hide {
   display: none;
}
.promoContainer {
    margin-top: 15px;
    padding-right: 20px;
    .left {
        margin-left: 0;
        width: 48%;

        .checkoutModule  {
            padding-right: 5%;


        }
    }

    .right {
        width:48%;
    }

    /* Sign in Form */
    #rememberme-label {
        display: inline;
    }

    #rememberme-element {
        display: inline;
        label {
           float: left;
        }
    }

    #rememberme {
        float: left;
    }

    label.required:after {
        content:"*";
        color:#F0141E;
    }

    .btn-blk {
        width: 85% !important;
        margin: 15px 0px;
        &:before, &:after {
            background-color:black;
        }
    }

    span.username-row-element input {
        margin-bottom:15px;
    }

    /* ERROR HANDLING FOR VALIDATION AND WELL ... USER ERRORS */

    .errorMessage {
        display:none;
        span {
            display: block;
            color: #F0141E !important;
            position: relative;
            top: -3px;
        }
        i {
            float: left;
            background-color: #F0141E !important;
            color: white;
            font-weight: bold;
            padding: 1px 6px;
            border-radius: 20px;
            font-size: 12px;
            font-style: normal;
            margin-right: 5px;
            margin-bottom: 10px;
        }
    }

    .errorWrapper {

        li {
           font-size:15px;
           color:#F0141E;
           margin-left:20px;
           line-height:26px;
           font-family: tradeGothic;
           list-style: none outside none;
       }

       li.icon-exclamation:before {
           font-family:'Marvelicons';
           position:absolute;
           left:0;
       }

    }

    .password_error #password,
    .username_error #username {
        border:1px solid #F0141E !important;
    }

    .username_error label.required,
    .password_error label.required {
        color:#F0141E !important
    }

    /* FORGOT PASSWORD SECTION */
    section.infoForm a.block {
        display: block;
        margin-bottom: 15px;
        text-decoration: none;
        &:after {
            content: '\00BB';
            padding-left: 5px;
            color: #F0141E;
        }
    }

    #forgotpwd a.btn {
        width:86%;
        margin-bottom: 20px;
    }

    section.forgot_password_cont {
        min-height: 100px;
    }

   /* CREATE an ACCOUNT section */
    .registerUser label {
        display:block;
    }

    .registerUser select {
        background: transparent;
        width: 133% !important;
        padding: 5px;
        font-size: 16px;
        line-height: 1;
        border: 0;
        border-radius: 0;
        height: 26px;
        -webkit-appearance: none;
        padding-top:1px;
        margin-top:0;
    }

    div.columnFieldboth  {
        height: 26px;
        overflow: hidden;
        background: url(/i/images/juggernaut/icons/iconArrows.png) no-repeat 92% -7px #fff;
        border: 1px solid #000;
        float: left;
        width:28%;
        margin-right:3%;
        position: relative;
        background: none;

        select {
            position: relative;
            z-index:100;
        }

        span {
            position: absolute;
            right:0;
            display: block;
            background: black;
            width:18px;
            height:100%;

            b {
                position: absolute;
                top: 47%;
                left: 25%;
                display: block;
                width: 0;
                height: 100%;
                color: #FFF;
                border-left: 5px solid transparent;
                border-right: 5px solid transparent;
                border-top: 5px solid #FFF;
            }
         }

    }

    div.columnFieldboth.error  {
        border:1px solid #F0141E;

        span {
            background:#F0141E;
        }
    }

    /* I FORGOT WHAT THIS IS BEFORE */
    #submit-label, #promocode-label {
        display:none;
    }

   #submit {
        margin: 0;
        padding: 0;
        border: none;
        background-color: black;
        display: block;
        padding: 0 10px;
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        text-align: center;
        white-space: nowrap;
        width:100%;
        height:47px;
        cursor: pointer;
        color: white !important;
        text-transform: uppercase;
        font-family: "TradeGothicBold";
        line-height: 47px;
        font-size: 15px;
        outline:0;
    }

    .left .checkoutModule input[type="text"],
    .checkoutModule input[type="password"],
    .checkoutModule input[type="email"] {
        width:90%;
    }

    fieldset > label {
        padding-bottom:6px;
    }

    /* TOOLTIP YO */
     div.tooltip.dialogHelp {
         right:auto;
         margin-top: auto;
         max-width:60%;
        .notch {
            top: 15px;
        }
     }
}


/* START OF MEDIA QUERIES */
@include media-baseline(0, $s-grid){
    .promoContainer {
          .right {
            width: 100%;
            float: none;
        }

        .left {
            width: 100%;
            float:none;
        }

        #promocode-element {
            width:50%;
        }
    }

}
@include media-baseline($s-grid, $m-grid){
        div.columnFieldboth {
            width: 92%;
            float: none;
        }

        .promo_redeem.left {
            width:100%;
        }

    .termsWrap label.choice {
        width: 81%;
    }

 }

@include media-baseline(0, $xs-grid) {
        #promocode {
            #promocode-element {
                width:90% !important;
            }

        a.btn-red {
           float: none;
           width: 90%;
        }
    }

    div.columnFieldboth {
            width: 92%;
            float: none;
        }

        .termsWrap label.choice {
            width: 81%;
        }
}

